#include "io8535v.h"
#include "macros.h"
void delay(int t)
{
	int c1,c2;
	for(c1=0;c1<t;c1++)
    {
    	for(c2=0;c2<970;c2++)
        {
        	NOP();
        }
    }
}
void main()
{
 	char c;
	char i;
	DDRC=0b11111111;
    while(1)
    {
	 	for(c=0;c<8;c++)
		{
		 	PORTC+=0b00000001<<c;
			delay(100);

		}
		for(c=0;c<8;c++)
		{
		 	PORTC+=~0b00000000<<c;
			delay(100);

		}
		for(c=0;c<8;c++)
		{
		 	PORTC+=0b10000000>>c;
			delay(100);

		}
		for(c=0;c<8;c++)
		{
		 	PORTC+=~0b11111111>>c;
			delay(100);

		}
		break;
    }
    
    
}